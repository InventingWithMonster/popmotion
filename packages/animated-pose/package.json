{
  "name": "animated-pose",
  "version": "0.2.2",
  "description": "Pose for the Animated animation library",
  "main": "lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "measure": "gzip -c dist/popmotion-pose.js | wc -c",
    "prettier": "prettier ./src/*",
    "prepublishOnly": "npm run prettier && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/popmotion"
  },
  "keywords": [
    "animation",
    "dom",
    "declarative",
    "popmotion"
  ],
  "author": "Matt Perry",
  "license": "MIT",
  "presets": [
    "env"
  ],
  "devDependencies": {
    "@types/jest": "^22.2.3",
    "jest": "^22.4.3",
    "prettier": "1.11.1",
    "react-native": "^0.55.4",
    "ts-jest": "^22.4.5",
    "typescript": "^2.8.3",
    "webpack": "^3.11.0"
  },
  "prettier": {
    "parser": "typescript",
    "singleQuote": true
  },
  "peerDependencies": {
    "react-native": "^0.55.4"
  },
  "dependencies": {
    "react-native": "^0.55.4",
    "@types/react-native": "^0.55.4",
    "pose-core": "^0.7.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "js"
    ],
    "transform": {
      "^.+\\.(js)$": "../node_modules/react-native/jest/assetFileTransformer.js",
      "\\.(ts)$": "../../../node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "/_tests/.*\\.(ts|js)$",
    "rootDir": "src"
  }
}
